        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="4*" />
        </Grid.RowDefinitions>
        <DataGrid ItemsSource="{Binding TeamAllTimeResults.YearByYearRecords}">
            
        </DataGrid>
    </Grid>
</UserControl>
